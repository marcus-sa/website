
            <section class="pt-page" data-id="home">
              <div class="section-inner start-page-content">
                <div class="page-header">
                  <div class="row">
                    <div class="col-sm-4 col-md-4 col-lg-4">
                      <div class="photo">
                        <img src="<%= profilePic %>" alt="">
                      </div>
                    </div>

                    <div class="col-sm-8 col-md-8 col-lg-8">
                      <div class="title-block">
                        <h1><%= firstName %> <%= lastName %></h1>
                        <div class="owl-carousel text-rotation">
                          <div class="item">
                            <div class="sp-subtitle">Frontend Developer</div>
                          </div>
                          <div class="item">
                            <div class="sp-subtitle">Backend Developer</div>
                          </div>
                        </div>
                      </div>

                      <div class="social-links">
                        <% Object.keys(social).forEach(name => { %>
                          <a href="<%= social[name] %>"><i class="fa fa-<%= name %>"></i></a>
                        <% }) %>
                      </div>
                    </div>
                  </div>
                </div>

                <div class="page-content">
                  <div class="row">

                    <div class="col-sm-6 col-md-6 col-lg-6">
                      <div class="about-me">
                        <div class="block-title">
                          <h3>About <span>Me</span></h3>
                        </div>
                        <p><%= description %></p>
                      </div>
                      <div class="download-resume">
                        <a href="" class="btn btn-secondary">Download Resume</a>
                      </div>
                    </div>

                    <div class="col-sm-6 col-md-6 col-lg-6">
                      <ul class="info-list">
                        <li><span class="title">Age</span><span class="value"><%= age %></span></li>
                        <li><span class="title">Residence</span><span class="value"><%= residence %></span></li>
                        <li><span class="title">Address</span><span class="value"><%= address %></span></li>
                        <li><span class="title">E-mail</span><span class="value"><a href="mailto:<%= email %>"><%= email %></a></span></li>
                        <li><span class="title">Phone</span><span class="value">+0123 123 456 789</span></li>
                        <li><span class="title">Freelance</span><span class="value available"><%= status %></span></li>
                      </ul>
                    </div>

                  </div>
                </div>
              </div>
            </section>
