                            <img src="http://lmpixels.com/demo/procard/images/portfolio/1.jpg" alt="">
                            <div>
                              <h5 class="name"><%= i.title %></h5>
                              <small>Media</small>
                              <i class="fa fa-file-text-o"></i>
                            </div>
                          </a>
                        </figure>
                      <% }) %>

